{
  "name": "icn-wallet-ui",
  "version": "0.1.0",
  "description": "ICN Wallet - Cross-platform DID and key management interface",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:web": "expo export:web",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "mobile:dev": "expo start",
    "mobile:build": "eas build --platform all",
    "preview": "expo start --tunnel",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .expo dist .tauri/target node_modules/.cache"
  },
  "dependencies": {
    "@icn/ui-kit": "workspace:*",
    "@icn/ts-sdk": "workspace:*",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@tamagui/animations-react-native": "^1.70.0",
    "@tamagui/config": "^1.70.0",
    "@tamagui/core": "^1.70.0",
    "@tamagui/font-inter": "^1.70.0",
    "@tamagui/theme": "^1.70.0",
    "expo": "~49.0.0",
    "expo-app-loading": "^2.1.0",
    "expo-constants": "~14.4.2",
    "expo-crypto": "~12.4.1",
    "expo-font": "~11.4.0",
    "expo-router": "^2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@expo/cli": "0.10.16",
    "@tauri-apps/api": "^1.5.0",
    "@tauri-apps/cli": "^1.5.0",
    "@types/react": "~18.2.0",
    "@types/react-native": "~0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.0"
  },
  "private": true,
  "keywords": [
    "icn",
    "wallet",
    "did",
    "identity",
    "react-native",
    "expo",
    "tamagui",
    "cross-platform"
  ],
  "author": "ICN Core Contributors",
  "license": "Apache-2.0"
} 